[all:vars]
ansible_ssh_common_args="-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null"
ansible_user=vagrant




[gateway]
inetRouter ansible_host=192.168.50.10 ansible_ssh_private_key_file=.vagrant/machines/inetRouter/virtualbox/private_key 


[services]
wordpress ansible_host=192.168.50.20 ansible_ssh_private_key_file=.vagrant/machines/wordpress/virtualbox/private_key
